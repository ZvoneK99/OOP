package while_03;

import java.util.Scanner;
public class while_03 {
	public static void main(String [] args) {
	
	Scanner ulaz = new Scanner(System.in);
	System.out.println("Unesite broj: ");
	int broj = ulaz.nextInt();
	int pocetak=1;
	while(pocetak<=broj) {
		System.out.println(pocetak);
		pocetak++;
	}
	}
}

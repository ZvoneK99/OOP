// primjer unosi ime i prezime (dvije rijeï¿½i) i razdvaja ih u zasebne stringove


package vjezba3;

import java.util.Scanner;

public class vjezba3 {
	
	public static void main(String[] args) {
		
		Scanner ulaz = new Scanner(System.in);
		System.out.println("Unesi ime i prezime");
		String ime_prezime = ulaz.nextLine();
		
		String ime = ime_prezime.substring(3, ime_prezime.indexOf(" "));
		System.out.println("Ime je: "+ime);
		String prezime = ime_prezime.substring(ime_prezime.indexOf(" ")+1);
		System.out.println("Prezime je: "+prezime);
	}
}
